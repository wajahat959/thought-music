{
  "expo": {
    "name": "thought_therapy",
    "slug": "thought_therapy",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/icons/log.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "thought"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icons/brain.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.wajahat959.thought_therapy"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-av",
        {
          "microphonePermission": "Allow $(thought_therapy) to access your microphone."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "17a250ea-4cc7-4edb-bf8b-b9514dbaeee4"
      }
    }
  }
}
